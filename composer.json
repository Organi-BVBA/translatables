{
    "name": "organi/translatables",
    "description": "",
    "keywords": [
        "Organi",
        "laravel",
        "translatables"
    ],
    "homepage": "https://bitbucket.org/organi_team/translatables",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Organi\\Translatables\\": "src",
            "Organi\\Translatables\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Organi\\Translatables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Organi\\Translatables\\TranslatablesServiceProvider"
            ],
            "aliases": {
                "Translatables": "Organi\\Translatables\\TranslatablesFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
