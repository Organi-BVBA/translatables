{
    "name": "organi/translatables",
    "description": "",
    "keywords": [
        "Organi",
        "laravel",
        "translatables"
    ],
    "homepage": "https://gitlab.com/Organi/translatables",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Van Keilegom",
            "email": "info@robvankeilegom.be",
            "homepage": "https://robvankeilegom.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "^8.37",
        "illuminate/database": "^8.50",
        "illuminate/support": "^8.50",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Organi\\Translatables\\": "src",
            "Organi\\Translatables\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Organi\\Translatables\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Organi\\Translatables\\TranslatablesServiceProvider"
            ],
            "aliases": {
                "Translatables": "Organi\\Translatables\\TranslatablesFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
